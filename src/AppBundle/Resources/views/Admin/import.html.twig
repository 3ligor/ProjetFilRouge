{% extends "AppBundle::layout.html.twig" %}

{% block content %}
    {% include "AppBundle:Admin:menuAdmin.html.twig" with {'active' : 'import'} %}
    <div class="row">
        <div class="panel panel-default">
            <div class="panel-heading">Importation d'un fichier .csv pour remplir la base</div>
            <div class="panel-body">
                <div class="col-lg-12">
					<div class="row">
						
							<form class="form-inline" action="" method="post" {{ form_enctype(form) }}>
								{{ form_start(form) }}  
								<div class="col-xs-4"> {{ form_widget(form.submitFile) }}</div>
								<div class="col-xs-8"> {{ form_widget(form.Valider) }} </div>
								{{ form_end(form) }}
							</form>
						 
					</div>
                </div>
            </div>
        </div>
    </div>
{% endblock content %}

